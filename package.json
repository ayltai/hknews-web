{
    "name": "hknews-web",
    "description": "Serves aggregated news from 10+ local news publishers in Hong Kong. Made with ❤",
    "version": "1.0.0",
    "author": "Alan Tai",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ayltai/hknews-web.git"
    },
    "bugs": {
        "url": "https://github.com/ayltai/hknews-web/issues"
    },
    "homepage": "https://hknews.dev/",
    "license": "Apache-2.0",
    "keywords": [
        "news",
        "newspaper",
        "hknews"
    ],
    "scripts": {
        "check": "./node_modules/.bin/stylelint src/**/*.css public/**/*.css !build/** && ./node_modules/.bin/tslint --project tsconfig.json src/**/*.{ts,tsx}",
        "codacy": "cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage",
        "codecov": "./node_modules/.bin/codecov",
        "serve": "./node_modules/.bin/serve -s build",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --ci --no-watchman --coverage --coverageDirectory=coverage --forceExit",
        "eject": "react-scripts eject"
    },
    "dependencies": {
        "@material-ui/core": "^4.5.0",
        "@material-ui/icons": "^4.4.3",
        "@material-ui/styles": "^4.5.0",
        "react": "^16.10.2",
        "react-admin": "^2.9.7",
        "react-dom": "^16.10.2",
        "react-image-lightbox": "^5.1.0",
        "react-jss": "^8.6.1",
        "react-scripts": "^3.2.0",
        "react-timeago": "^4.4.0",
        "typescript": "^3.6.3",
        "@types/node": "^12.7.12",
        "@types/react-dom": "^16.9.1",
        "@types/react-jss": "^8.6.4",
        "@types/react-timeago": "^4.1.1"
    },
    "devDependencies": {
        "codacy-coverage": "^3.4.0",
        "codecov": "^3.6.1",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.14.0",
        "serve": "^11.2.0",
        "stylelint": "^11.0.0",
        "stylelint-config-standard": "^19.0.0",
        "tslint": "^5.20.0",
        "tslint-sonarts": "^1.9.0",
        "@types/enzyme": "^3.10.3",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/jest": "^24.0.18",
        "@types/material-ui": "^0.21.7",
        "@types/react": "^16.9.5"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
